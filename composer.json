{
    "name": "categor-product/laravel-crud",
    "description": "A Laravel package for Category and Product CRUD",
    "type": "library",
    "autoload": {
        "psr-4": {
            "MyCrud\\": "src/"
        }
    },
    "require": {
        "php": "^7.3|^8.4",
        "illuminate/support": "^7.0|^8.0|^9.0|^11.0|^12.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/abdugoffor/crud-package"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "MyCrud\\MyCrudServiceProvider"
            ]
        }
    }
}